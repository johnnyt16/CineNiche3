{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MoviesDb": "Data Source=Data/Movies.db"
  },
  "AzureBlobStorage": {
    "ConnectionString": "YOUR_AZURE_CONNECTION_STRING",
    "ContainerName": "postersintex",
    "BaseUrl": "https://postersintex.blob.core.windows.net/postersintex/"
  },
  "Stytch": {
    "ProjectID": "YOUR_STYTCH_PROJECT_ID",
    "Secret": "YOUR_STYTCH_SECRET",
    "PublicToken": "YOUR_STYTCH_PUBLIC_TOKEN",
    "BaseUrl": "https://test.stytch.com/v1/",
    "JwtSigningKey": "YOUR_JWT_SIGNING_KEY",
    "JwtExpiryMinutes": 60,
    "LoginRedirectUrl": "http://localhost:3000/login",
    "SignupRedirectUrl": "http://localhost:3000/register"
  },
  "Jwt": {
    "Secret": "YOUR_JWT_SECRET",
    "Issuer": "CineNiche.API",
    "Audience": "CineNicheApp",
    "ExpiryInMinutes": 60
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5212"
      },
      "Https": {
        "Url": "https://localhost:5213"
      }
    }
  }
}
